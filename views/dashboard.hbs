<section id="mainPanel">
    <h2>Welcome {{characterName}}, the {{job}}</h2>
    <p>Here you can manage your inventory, quests, and clan.</p>
    <p>What would you like to do?</p>
</section>

<nav id="dashboardNav">
    <button id="showCharacter" hx-get="/showCharacter" hx-select="#characterPanel" hx-target="#mainPanel">Character</button>
    <button id="showInventory" hx-get="/showInventory" hx-select="#inventoryPanel" hx-target="#mainPanel">Inventory</button>
    <button id="showQuests" hx-get="/showQuests" hx-target="#mainPanel">Quests</button>
    <button id="showClan" hx-get="/showClan" hx-target="#mainPanel">Clan</button>
</nav>

 